
function palindrome(str) {
  // Good luck!
  str = str.replace(/[^a-zA-Z0-9]/g,'');
  str = str.toLowerCase();
  str = str.split("");
  var newStr = str;
    str = str.join("");
var reversed = newStr.reverse();
 reversed = reversed.join("");

if (str == reversed){
  return true;
   }else{return false;}
}



palindrome("not a palindrome");
